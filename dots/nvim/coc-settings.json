{
  // Coc
  "coc.preferences.formatOnSaveFiletypes": [
    "c",
    "cpp",
    "cs",
    "csharp",
    "css",
    "go",
    "html",
    "java",
    "javascript",
    "javascriptreact",
    "json",
    "jsonc",
    "kotlin",
    "latex",
    "lua",
    "markdown",
    "nix",
    "prisma",
    "python",
    "rust",
    "scala",
    "scss",
    "svg",
    "toml",
    "typescript",
    "typescriptreact",
    "vim",
    "vim",
    "vue",
    "xml",
    "yaml"
  ],
  "omnisharp.path": "omnisharp",
  "languageserver": {
    "nix": {
      "command": "rnix-lsp",
      "filetypes": ["nix"]
    }
  },
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextPrefix": "> ",
  "diagnostic.refreshOnInsertMode": true,
  "suggest.floatEnable": true,
  "suggest.enablePreview": true,
  "suggest.localityBonus": true,
  "suggest.fixInsertedWord": true,
  "suggest.snippetsSupport": true,
  "coc.preferences.colorSupport": true,
  "coc.preferences.bracketEnterImprove": true,
  "coc.preferences.rootPatterns": ["package.json"],
  "coc.source.emoji.filetypes": [
    "markdown",
    "javascript",
    "typescript",
    "gitcommit"
  ],
  "coc.preferences.watchmanPath": "asdfa",
  "coc.source.file.enable": true,
  "suggest.echodocSupport": false,
  "suggest.autoTrigger": "always",
  "codeLens.enable": false,
  "snippets.userSnippetsDirectory": "~/.config/nvim/snippets/user",
  // Rust
  "rust-analyzer.inlayHints.chainingHints": false,
  "rust-analyzer.cargo.allFeatures": true,
  "rust-analyzer.cargo.autoreload": true,
  "rust-analyzer.procMacro.enable": true,
  // Yaml
  "yaml.format.enable": true,
  "yaml.schemas": {},
  "yaml.schemaStore.enable": true,
  // Metals
  "metals.statusBarEnabled": true,
  // Eslint
  "eslint.autoFixOnSave": true,
  "eslint.packageManager": "yarn",
  // Vetur
  "vetur.validation.style": false,
  "vetur.format.enable": true,
  // CSS
  "css.validate": false,
  "scss.validate": false,
  // Typescript/Javascript
  "javascript.preferences.importModuleSpecifier": "relative",
  "typescript.preferences.importModuleSpecifier": "relative",
  "tsserver.log": "verbose",
  "tsserver.maxTsServerMemory": 4096,
  "tsserver.watchOptions": {
    "watchFile": "fixedPollingInterval",
    "watchDirectory": "fixedPollingInterval",
    "fallbackPolling": "priorityPollingInterval",
    "synchronousWatchDirectory": true
  },
  "typescript.suggest.importStatements": true,
  "typescript.suggest.autoImports": true,
  "typescript.suggest.includeCompletionsForImportStatements": true,
  "tsserver.disableAutomaticTypeAcquisition": false,
  "tsserver.implicitProjectConfig.checkJs": false,
  "javascript.updateImportsOnFileMove.enable": false,
  "typescript.updateImportsOnFileMove.enable": false,
  // Emmet
  "emmet.includeLanguages": {
    "vue-html": "html",
    "javascript": "javascriptreact",
    "jsx": "javascriptreact",
    "tsx": "typescriptreact",
    "javascriptreact": "javascriptreact",
    "typescriptreact": "typescriptreact"
  },
  // Snippets
  "snippets.extends": {
    "cpp": ["c"],
    "javascriptreact": ["javascript"],
    "typescript": ["javascript", "javascript-jsdoc"],
    "typescriptreact": ["typescript", "javascript", "javascript-jsdoc"]
  },
  // Python
  // "python.pythonPath": "~/dotfiles/python-picker",
  "python.formatting.provider": "black",
  "python.linting.pylintEnabled": true,
  "pyright.organizeimports.provider": "pyright",
  "pyright.completion.snippetSupport": true,
  "python.analysis.autoImportCompletions": true,
  // Go
  "go.goplsOptions": {
    "completeUnimported": true,
    "usePlaceholders": true
  },
  "go.goplsPath": "gopls",
  // Java
  "java.errors.incompleteClasspath.severity": "ignore",
  // Lua
  "Lua.workspace.library": {
    "/usr/share/nvim/runtime/lua": true,
    "/usr/share/nvim/runtime/lua/vim": true,
    "/usr/share/nvim/runtime/lua/vim/lsp": true
  },
  "Lua.diagnostics.globals": ["vim"],
  // TailwindCSS
  "tailwindCSS.htmlLanguages": [
    "HTML (EEx)",
    "HTML (Eex)",
    "blade",
    "edge",
    "eelixir",
    "ejs",
    "elixir",
    "elm",
    "erb",
    "eruby",
    "haml",
    "handlebars",
    "html",
    "html.twig",
    "jade",
    "javascriptreact",
    "jsx",
    "typescriptreact",
    "tsx",
    "leaf",
    "markdown",
    "njk",
    "nunjucks",
    "php",
    "razor",
    "slim",
    "svelte",
    "twig",
    "vue",
    "pug"
  ],
  "tailwindCSS.experimental.classRegex": [
    "tw`([^`]*)", // tw`...`
    "tw\\..+`([^`]*)", // tw.xxx<xxx>`...`
    "tw\\(.*?\\).*?`([^`]*)" // tw(Component)<xxx>`...`
  ],
  // "explorer.icon.enableVimDevicons": true,
  "explorer.icon.enableNerdfont": true,
  "explorer.quitOnOpen": true,
  "explorer.position": "right",
  "explorer.previewAction.onHover": false,
  "explorer.keyMappings.global": {
    "i": "cd",
    "gk": "expandablePrev",
    "gj": "expandableNext",
    "*": "toggleSelection",
    "<tab>": "actionMenu",
    "h": "collapse",
    "J": ["wait", "toggleSelection", "normal:j"],
    "K": ["wait", "toggleSelection", "normal:k"],
    "O": "expand:recursive",
    "X": "collapse:recursive",
    "<2-LeftMouse>": [
      "expandable?",
      ["expanded?", "collapse", "expand"],
      "open"
    ],
    "o": ["wait", "expanded?", "collapse", "expand"],
    "<cr>": [
      "wait",
      "expandable?",
      ["expanded?", "collapse", "expand"],
      "open"
    ],
    "l": ["wait", "expandable?", ["expanded?", "collapse", "expand"], "open"],
    "e": "open",
    "s": "open:split",
    "S": "open:split:plain",
    "E": "open:vsplit",
    "t": "open:tab",
    "u": "gotoParent",
    "gp": "preview:labeling",
    "yp": "copyFilepath",
    "yn": "copyFilename",
    "yy": "copyFile",
    "dd": "cutFile",
    "p": "pasteFile",
    "D": "deleteForever",
    "a": "addFile",
    "A": "addDirectory",
    "r": "rename",
    ".": "toggleHidden",
    "R": "refresh",
    "?": "help",
    "q": "quit",
    "<esc>": "esc",
    // "X": "systemExecute",
    "gd": "listDrive",
    "f": "search",
    "F": "searchRecursive",
    "gf": "gotoSource:file",
    "gb": "gotoSource:buffer",
    "[[": "sourcePrev",
    "]]": "sourceNext",
    "[d": "diagnosticPrev",
    "]d": "diagnosticNext",
    "[c": "gitPrev",
    "]c": "gitNext",
    "<<": "gitStage",
    ">>": "gitUnstage"
  }
}
